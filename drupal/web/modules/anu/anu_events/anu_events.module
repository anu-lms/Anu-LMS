<?php

use Drupal\anu_events\AnuEventBase;

/**
 * @file
 * Contains hooks definition for Anu Events module.
 */

/**
 * Implements hook_entity_insert().
 *
 * @param \Drupal\Core\Entity\EntityInterface $entity
 */
function anu_events_entity_insert(Drupal\Core\Entity\EntityInterface $entity) {
  AnuEventBase::triggerAnuEvents('entity_insert', ['entity' => $entity]);
}
